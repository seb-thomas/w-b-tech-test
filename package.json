{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "db": "json-graphql-server server/db.js --p 3022",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run db & npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "npm run test:e2e",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:open"
  },
  "dependencies": {
    "@apollo/client": "^3.1.2",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^4.25.7",
    "gatsby-plugin-apollo": "^3.0.1",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typography": "^2.5.10",
    "json-graphql-server": "^2.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.1.1",
    "typography": "^0.16.19",
    "typography-theme-fairy-gates": "^0.16.19",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "cypress": "^4.12.1",
    "prettier": "2.0.5",
    "start-server-and-test": "^1.11.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
